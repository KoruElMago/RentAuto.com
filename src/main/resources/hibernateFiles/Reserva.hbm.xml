<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="model.reserva" table="reservas" >
        <id name="id" type="java.lang.integer" column="id">
            <generator class="increment" />
        </id>
    <property name="numeroSolicitud" type="integer" column="numeroSolicitud"/>
	<property name="inicio" type="date" column="inicio"/>
	<property name="fin" type="date" column="fin"/>
	<many-to-one name="origen" column="origen" class="model.Ubicacion"/>
	<many-to-one name="destino" column="destino" class="model.Ubicacion"/>
	<many-to-one name="auto" column="auto" class="model.Auto"/>
	<many-to-one name="usuario" column="usuario" class="model.Usuario"/>
    </class>
</hibernate-mapping>