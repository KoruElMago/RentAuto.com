<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="model.reserva" table="reservas" >
        <id name="numeroSolicitud" type="java.lang.integer" column="id">
            <generator class="identity" />
        </id>
	<many-to-one name="origen" column="origen" class="Ubicacion"/>
	<many-to-one name="destino" column="destino" class="Ubicacion"/>
	<many-to-one name="auto" column="auto" class="Auto"/>
	<many-to-one name="usuario" column="usuario" class="Usuario"/>
    </class>
</hibernate-mapping>